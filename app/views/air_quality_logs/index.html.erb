<h1>Listing air_quality_logs</h1>

<table>
  <thead>
    <tr>
      <th>Sensor</th>
      <th>Value</th>
      <th>Date time</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @air_quality_logs.each do |air_quality_log| %>
      <tr>
        <td><%= air_quality_log.sensor %></td>
        <td><%= air_quality_log.value %></td>
        <td><%= air_quality_log.date_time %></td>
        <td><%= link_to 'Show', air_quality_log %></td>
        <td><%= link_to 'Edit', edit_air_quality_log_path(air_quality_log) %></td>
        <td><%= link_to 'Destroy', air_quality_log, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Air quality log', new_air_quality_log_path %>
